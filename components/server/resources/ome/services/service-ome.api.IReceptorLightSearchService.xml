<beans>
  <bean parent="level1" id="internal-ome.api.IReceptorLightSearchService" class="ome.logic.ReceptorLightSearchServiceImpl">
    <property name="preferenceContext" ref="preferenceContext"/>
  </bean>
  
  <bean id="managed-ome.api.IReceptorLightSearchService" parent="managedService">
    <property name="proxyInterfaces">
      <list>
	<value>ome.api.IReceptorLightSearchService</value>
      </list>
    </property>
    <property name="target" ref="internal-ome.api.IReceptorLightSearchService"/>
  </bean>
</beans>
    