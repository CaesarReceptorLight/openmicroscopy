<ivy-module version="1.0">
  <info organisation="omero" module="main" revision="${omero.version}" status="integration"/>
  <configurations>
    <conf name="build"/>
    <conf name="client"/>
    <conf name="server"/>
  </configurations>
  <publications>
    <artifact name="main" type="zip"/>
  </publications>
  <dependencies>
    <dependency org="org.openmicroscopy" name="omero-blitz" rev="${omero.version}"/>
    <dependency name="insight" rev="${omero.version}" changing="true" conf="build,client->client"/>
    <!-- runtime dependencies from dsl/ivy.xml -->
    <dependency org="janino" name="janino" rev="${versions.janino}"/>
    <dependency org="org.perf4j" name="perf4j" rev="${versions.perf4j}"/>
  </dependencies>
</ivy-module>
